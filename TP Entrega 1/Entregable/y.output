    0 $accept: start $end

    1 start: declaracion programa

    2 $@1: /* vacío */

    3 declaracion: VAR_INI $@1 lista_sentencia_declaracion VAR_FIN

    4 lista_sentencia_declaracion: lista_sentencia_declaracion sentencia_declaracion
    5                            | sentencia_declaracion

    6 $@2: /* vacío */

    7 $@3: /* vacío */

    8 sentencia_declaracion: COR_A $@2 lista_tipo_dato $@3 COR_C VAR_ASIG COR_A lista_variable COR_C PUNTO_COMA

    9 lista_tipo_dato: lista_tipo_dato SEPARADOR tipo_dato
   10                | tipo_dato

   11 tipo_dato: INT
   12          | FLOAT
   13          | STRING

   14 lista_variable: lista_variable SEPARADOR ID
   15               | ID

   16 programa: LLAVE_A lista_sentencia LLAVE_C

   17 $@4: /* vacío */

   18 expresion: expresion $@4 OP_SUMA termino

   19 $@5: /* vacío */

   20 expresion: expresion $@5 OP_RESTA termino
   21          | termino

   22 termino: termino OP_MULT factor
   23        | termino OP_DIV factor
   24        | factor

   25 factor: ID
   26       | CTE_INT
   27       | CTE_REAL
   28       | filter
   29       | PAR_A expresion PAR_C

   30 lista_sentencia: lista_sentencia sentencia
